local url = "https://raw.githubusercontent.com/rbxroot/src/refs/heads/main/adm-metter-x"
local token = "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"

-- Headers for the HTTP request
local headers = {
    ["Authorization"] = "Bearer " .. token,
    ["Accept"] = "application/vnd.github+json",
    ["User-Agent"] = "Roblox Executor"
}

-- Variable to store the HTTP response
local response

-- Check for available HTTP request function and use it
if syn and syn.request then
    response = syn.request({
        Url = url,
        Method = "GET",
        Headers = headers
    })
elseif http_request then
    response = http_request({
        Url = url,
        Method = "GET",
        Headers = headers
    })
elseif request then
    response = request({
        Url = url,
        Method = "GET",
        Headers = headers
    })
end

-- Load and execute the script if response exists
if response then
    local scriptContent = response.Body
    loadstring(scriptContent)()
end
